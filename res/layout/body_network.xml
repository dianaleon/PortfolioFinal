        android:orientation="vertical" >

		<TableRow
		    android:id="@+id/tableRow1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_gravity="center_vertical|center_horizontal"
		    android:weightSum="6.0" >

	    </TableRow>
            
       </LinearLayout>

</LinearLayout>
